{
  "name": "LibWally",
  "version": "0.0.1",
  "summary": "Swift wrapper for LibWally.",
  "description": "Swift wrapper for LibWally, a collection of useful primitives for cryptocurrency wallets.",
  "homepage": "https://github.com/Sjors/libwally-swift",
  "license": {
    "type": "MIT",
    "file": "LICENSE.md"
  },
  "authors": {
    "Sjors Provoost": "sjors@sprovoost.nl"
  },
  "platforms": {
    "ios": "11"
  },
  "swift_versions": "5.0",
  "source": {
    "git": "https://github.com/Sjors/libwally-swift.git",
    "tag": "v0.0.1",
    "submodules": true
  },
  "pod_target_xcconfig": {
    "SWIFT_WHOLE_MODULE_OPTIMIZATION": "YES",
    "SWIFT_INCLUDE_PATHS": "$(PODS_TARGET_SRCROOT)/build"
  },
  "preserve_paths": [
    "LibWally/LibWally.modulemap",
    "build"
  ],
  "module_map": "LibWally/LibWally.modulemap",
  "prepare_command": "./build-libwally-swift.sh",
  "vendored_frameworks": "build/LibWally.xcframework",
  "swift_version": "5.0"
}
